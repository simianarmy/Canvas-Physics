<% content_for(:head) do %>
  <link href="css/rotate.css" rel="stylesheet" type="text/css" media="screen" />
<% end %>
<div id="container">
  <header>
    <h1 class="title">Canvas: Rotating objects collison detection</h1>
  </header>
  <div id="main" role="main">
    <ul id="controls">
      <li>
        Starting angle: <input name="sAngle" default="90" size="3" value="90"/> degrees
      </li>
      <li>
        Angular velocity: <input name="angVel" size="3" value="15"/> degrees/second
      </li>
      <li>
        <input type="checkbox" name="movingBall" value="1"> Moving ball
      </li>
    </ul>
    Click on canvas to play or pause
    <canvas id="maincanvas" width="800" height="600"></canvas>
    <div id="info">
    </div>
  </div>
  <footer>
    Source: 
    <a href="docs/rot_collisions.js.html">collisions</a>
  </footer>
</div> <!--! end of #container -->
<script defer src="js/rot_collisions.js"></script>